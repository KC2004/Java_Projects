
public class List {
	int[] list;
	/**
	 * constructor
	 */
	public List(){
		
	}
	
	/**
	 * constructor with listSize as parameter
	 */
	public List(int listSize){
		list = new int[listSize];
	}
	
	/**
	 * returns a string with all values of list
	 */
	public String toString(){
		String strList = ""; 
		for (int i=0; i<list.length; i++){
			strList += list[i] + ",";
		}
		return strList;
	}
}
